//
// Proper nouns.
//

general construction ProperNounNP
    subcase of NP, Word


construction JOHN
    subcase of ProperNounNP, Singular, ThirdPerson
    constructional
    form
        constraints
            self.f.orth <-- "John"
    meaning
        constraints
            self.m.referent <-- @john-instance
            self.m.ontological-category <-- @male-person
      		self.m.number <-- "1"


