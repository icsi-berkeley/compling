//
// Mass Nouns
//

general construction MassNoun
  subcase of Noun, Singular
   constructional
     constraints
       self.features.massOrPlural <-- "yes"  


construction Mud
  subcase of MassNoun
   form
      constraints
         self.f.orth <-- "mud"
   meaning
    constraints
      self.m.ontological-category <-- @mud

