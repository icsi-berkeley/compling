package robots
import starter

construction NEAR-Locative
   subcase of LocativePreposition 
   form
      constraints
         self.f.orth <-- "near"
   meaning 
      evokes Proximity as p
      constraints
       self.m.landmark.ontological-category <--@region
       self.m.profiledArea <--> p.proximalArea 
       self.m.landmark <--> p.center   

	
	          